@model IEnumerable<Product>
@{
    Layout = "_Layout";
}
 @if (Model.Count()==0)
    {
    <h1 class="text-center p-5">У нас нет таких товаров :(</h1>
}
else
{
   <div class="row row-cols-1 row-cols-md-3 g-4 mt-5">

        @foreach (var item in Model)
        {
            @await Html.PartialAsync("Product",item)
        }
</div> 
}
